{
  "name": "WorkWide",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 14'",
    "start": "react-native start --resetCache",
    "test": "jest",
    "lint": "eslint .",
    "iosxcode": "open ios/WorkWide.xcworkspace",
    "generateapk": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res && cd android && gradlew assembleDebug && cd ../"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "axios": "^1.3.6",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-image-crop-picker": "0.37.3",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-svg": "12.1.1",
    "twrnc": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.1",
    "@babel/runtime": "^7.22.3",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-jest": "^29.5.0",
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "metro-react-native-babel-preset": "^0.76.5",
    "react-test-renderer": "17.0.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
